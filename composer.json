{
  "name": "mrmishmash/grumphp-parallel-phpcs",
  "description": "An extension that basically copies the Phpcs task from GrumPHP and adds parallel support.",
  "license": "MIT",
  "authors": [
    {
      "name": "Mihael Wagner",
      "email": "miha.wagner@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "phpro/grumphp": "^2.8",
    "symfony/process": "^6.4",
    "symfony/options-resolver": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Mishmash\\GrumPHPParallelPhpCs\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  }
}
